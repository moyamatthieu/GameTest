{
  "name": "jeux_gestion",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.{js,css,html}\"",
    "index-code": "node dev-tools/index-codebase.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev --prefix server\"",
    "qdrant:up": "docker-compose up -d",
    "qdrant:down": "docker-compose down",
    "qdrant:logs": "docker-compose logs -f",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:e2e": "playwright test",
    "test": "npm run test:unit && npm run test:e2e",
    "check-types": "tsc --build",
    "monitor": "node server/monitoring/MetricsCollector.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/three": "^0.182.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.1.2",
    "@qdrant/js-client-rest": "^1.13.0",
    "protobufjs": "^8.0.0",
    "simplex-noise": "^4.0.3",
    "socket.io-client": "^4.8.3",
    "three": "^0.182.0"
  }
}
